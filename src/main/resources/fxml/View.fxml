<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>


<BorderPane fx:id="root" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1">
   <bottom>
      <HBox spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="importarButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Importar" HBox.hgrow="ALWAYS" />
            <Button fx:id="eliminarButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Eliminar" HBox.hgrow="ALWAYS" />
            <Button fx:id="exportarButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Exportar" HBox.hgrow="ALWAYS" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </bottom>
   <center>
      <ListView fx:id="lista" BorderPane.alignment="CENTER" />
   </center>
   <padding>
      <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
   </padding>
</BorderPane>
